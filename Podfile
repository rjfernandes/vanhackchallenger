  pod 'RxCocoa', '~> 4.0'
  pod 'Kingfisher', '~> 4.0'
  pod 'SwiftDate'

  target 'HackatonTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'HackatonUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
